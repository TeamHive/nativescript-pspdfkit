{
  "name": "nativescript-pspdfkit",
  "version": "2.3.1",
  "description": "Nativescript implementation of PSPDFKit.",
  "main": "pspdfkit",
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
    "demo.android": "npm run preparedemo && cd demo && tns emulate android",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-pspdfkit && tns plugin add .. && tns install",
    "setup": "cd demo && npm install && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sean-perkins/nativescript-pspdfkit.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS",
    "PSPDFKit",
    "Typescript"
  ],
  "author": {
    "name": "Sean Perkins",
    "email": "sean@meetmaestro.com"
  },
  "contributors":  [
    {
      "name": "Osei Fortune",
      "email": "fortune.osei@yahoo.com",
      "url": "https://github.com/triniwiz"
    }
  ],
  "bugs": {
    "url": "https://github.com/sean-perkins/nativescript-pspdfkit/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/sean-perkins/nativescript-pspdfkit",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-platform-declarations": "^3.0.0",
    "tns-core-modules": "^3.0.0",
    "typescript": "^2.2.0"
  },
  "peerDependencies": {
    "tns-core-modules": "^3.0.0"
  }
}