<Page
    xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded"
    xmlns:pdfView="nativescript-pspdfkit">
    <StackLayout>
        <Button text="Get Name" tap="{{getName}}"/>
        <Button text="Fill Form" tap="{{fillForm}}"/>
        <Button text="Clear Form" tap="{{clearForm}}"/>
        <Button text="Form Viewer" tap="{{formViewer}}"/>
        <Button text="Second Viewer" tap="{{secondViewer}}"/>
        <Button text="Destroy Viewer" tap="{{destroyViewer}}"/>
        <pdfView:TNSPSPDFView  backgroundSaving="false" undoEnabled="false" scrollDirection="vertical" formsEnabled="false" id="pdf" row="1" src="https://www.ets.org/Media/Tests/GRE/pdf/gre_research_validity_data.pdf"></pdfView:TNSPSPDFView>
    </StackLayout>
</Page>
