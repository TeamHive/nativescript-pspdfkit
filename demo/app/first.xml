<Page 
    xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" 
    xmlns:pdfView="nativescript-pspdfkit">
    <StackLayout>
        <Button text="Get Name" tap="{{getName}}"></Button>
        <Button text="Fill Form" tap="{{fillForm}}"></Button>
        <Button text="Clear Form" tap="{{clearForm}}"></Button>
        <Button text="Second Viewer" tap="{{secondViewer}}"/>
        <Button text="Destroy Viewer" tap="{{destroyViewer}}"/>
        <pdfView:TNSPSPDFView cache="nothing" backgroundSaving="false" undoEnabled="false" pageMode="single" scrollDirection="vertical" formsEnabled="false" thumbnailBar="none" id="pdf" row="1" src="https://www.ets.org/Media/Tests/GRE/pdf/gre_research_validity_data.pdf"></pdfView:TNSPSPDFView>
    </StackLayout>
</Page>